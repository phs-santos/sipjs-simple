{
  "name": "@phs-santos/sipjs-simple",
  "version": "1.0.0",
  "description": "Uma biblioteca que simplifica a API do SIP.js, tornando mais fácil a integração e o uso de funcionalidades SIP em seus projetos.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "deploy": "npm publish"
  },
  "keywords": [
    "sip",
    "voip",
    "webrtc",
    "sip.js"
  ],
  "author": "Paulo Souza <paulo18santos2000@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.10.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "sip.js": "^0.21.2"
  }
}
